{
  "name": "vue-plugin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "server": "nodemon ./server/app.js"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "koa": "^2.7.0",
    "koa-router": "^7.4.0",
